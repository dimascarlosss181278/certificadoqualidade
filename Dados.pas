uses CertAnidro, certificado, CertHidrat;

{$R *.dfm}

procedure TDmdados.DataModuleCreate(Sender: TObject);
begin
   If ADOQCertificado.Active = False then ADOQCertificado.Active := True;
   if ADOQSD2010.Active = False then ADOQSD2010.Active := True;
   if ADOQFunc.Active = False then ADOQFunc.Active := True;
   If ADOQEstoquesaida.Active = False then ADOQEstoquesaida.active := True;
   if ADOQSB1010.Active = False then ADOQSB1010.Active := True;
end;

procedure TDmdados.DataModuleDestroy(Sender: TObject);
begin
   ADOQCertificado.Active := False;
end;

procedure TDmdados.ADOQCertificadoAfterInsert(DataSet: TDataSet);
begin
 if tipo='A' then begin
    certAnidro.Anidro.DBEdit1.Text  := DateToStr(Date);
    certAnidro.Anidro.DBEdit18.Text := '2013/2014';
    certanidro.Anidro.DBEdit21.Text := 'ALCOOL ANIDRO';
    certanidro.Anidro.DBEdit20.Text := ADOQSD2010D2_DOC.AsString;
    ADOQCertificadoCERT_PRODUTO.AsString := '010001';
    Anidro.PageControl1.ActivePageIndex := 1;
    certanidro.Anidro.DBEdit17.SetFocus;
    certanidro.Anidro.DBEdit1.Text  := DateToStr(Date)
end
 else if tipo = 'H' then begin
    certhidrat.HIDRAT.DBEdit1.Text  := DateToStr(Date);
    certhidrat.HIDRAT.DBEdit18.Text := '2013/2014';
    certhidrat.HIDRAT.DBEdit21.Text := 'ALCOOL HIDRATADO';
    certhidrat.HIDRAT.DBEdit20.Text := ADOQSD2010D2_DOC.AsString;
    ADOQCertificadoCERT_PRODUTO.AsString := '010002';
    certhidrat.HIDRAT.PageControl1.ActivePageIndex := 1;
    certhidrat.HIDRAT.DBEdit17.SetFocus;
    certhidrat.HIDRAT.DBEdit1.Text  := DateToStr(Date);
 end;
    Dmdados.ADOQCertificado.FieldByName('CERT_DTEXCLUSAO').AsString := '';
    Dmdados.ADOQCertificado.FieldByName('CERT_USUEXCLUSAO').AsString := '';
end;

end.
